# Docker Environment Configuration for Qwen3-VL Inference Server
# Copy this file to .env and customize for your deployment

# ============================================================================
# Model Configuration
# ============================================================================
MODEL_PATH=Qwen/Qwen3-VL-235B-A22B-Instruct
# Or use local model path (must be mounted in docker-compose.yml):
# MODEL_PATH=/models/Qwen3-VL-235B-A22B-Instruct

# HuggingFace Token (required for gated models)
# Get your token from: https://huggingface.co/settings/tokens
HF_TOKEN=

# ============================================================================
# Server Configuration
# ============================================================================
HOST_PORT=8000
DEBUG=False

# ============================================================================
# GPU Configuration
# ============================================================================
# GPU memory utilization (0.0 to 1.0)
GPU_MEMORY_UTILIZATION=0.70

# Tensor parallel size (number of GPUs to use)
# Leave empty for auto-detection
TENSOR_PARALLEL_SIZE=

# ============================================================================
# Inference Configuration
# ============================================================================
DEFAULT_MAX_TOKENS=2048
DEFAULT_TEMPERATURE=0.0
DEFAULT_TOP_P=1.0

# ============================================================================
# Image Processing Configuration
# ============================================================================
DEFAULT_MIN_PIXELS=65536
DEFAULT_MAX_PIXELS=2097152

# ============================================================================
# Video Processing Configuration
# ============================================================================
DEFAULT_MAX_FRAMES=2048
DEFAULT_SAMPLE_FPS=2.0
DEFAULT_TOTAL_PIXELS=20971520

# ============================================================================
# CORS Configuration
# ============================================================================
ALLOW_ORIGINS=*
ALLOW_CREDENTIALS=True
ALLOW_METHODS=*
ALLOW_HEADERS=*
